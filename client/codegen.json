{
    "overwrite": true,
    "schema": "http://localhost:8080",
    "documents": null,
    "generates": {
        "./graphql/generated/types.ts": {"plugins": ["typescript"]},
        "./graphql/generated/queries.ts": {
            "plugins": ["typescript", "typescript-operations"],
            "documents": [
                "./graphql/queries/queries.ts",
                "./containers/AquascapeDetailsContainer/queries.ts",
                "./containers/AquascapeDetailsEditContainer/queries.ts",
                "./containers/ProfileContainer/queries.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/HardscapeSectionContainer/queries.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/LivestockSectionContainer/queries.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/PlantsSectionContainer/queries.ts"
            ],
            "config": ["flattenGeneratedTypes"]
        },
        "./graphql/generated/mutations.ts": {
            "plugins": ["typescript", "typescript-operations"],
            "documents": [
                "./graphql/mutations/mutations.ts",
                "./containers/AquascapeDetailsContainer/CommentsContainer/mutations.ts",
                "./containers/AquascapeDetailsEditContainer/HeroSectionEditContainer/mutations.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/mutations.ts",
                "./containers/AquascapeDetailsEditContainer/PhotoSectionEditContainer/mutations.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/HardscapeSectionContainer/mutations.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/LivestockSectionContainer/mutations.ts",
                "./containers/AquascapeDetailsEditContainer/FloraSectionEditContainer/PlantsSectionContainer/mutations.ts"
            ]
        }
    }
}
